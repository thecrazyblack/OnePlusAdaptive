    <item component="ComponentInfo{com.android.documentsui/com.android.documentsui.LauncherActivity}" drawable="oneplus_files" />
    <item component="ComponentInfo{com.android.gallery3d/com.android.gallery3d.app.Gallery}" drawable="oneplus_gallery" />
