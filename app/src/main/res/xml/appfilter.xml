    <item component="ComponentInfo{com.android.camera2/com.android.camera.CameraLauncher}" drawable="oneplus_camera"/>
    <item component="ComponentInfo{com.android.deskclock/com.android.deskclock.DeskClock}" drawable="oneplus_clock" />
    <item component="ComponentInfo{com.android.contacts/com.android.contacts.activities.PeopleActivity}" drawable="oneplus_contacts" />
    <item component="ComponentInfo{com.android.dialer/com.android.dialer.DialtactsActivity}" drawable="oneplus_dialer" />
