<?xml version="1.0" encoding="utf-8"?>
<resources>

    <!-- OnePlus Dynamic Clock -->
    <dynamic-clock drawable="oneplus_clock"
        hourLayerIndex="0"
        minuteLayerIndex="1"
        secondLayerIndex="2"
        defaultHour="10"
        defaultMinute="10"
        defaultSecond="30" />


    <item component="ComponentInfo{com.google.android.deskclock/com.android.deskclock.DeskClock}" drawable="oneplus_clock" />

    <!-- Global -->
    <item component="ComponentInfo{com.android.calendar/com.android.calendar.AllInOneActivity}" drawable="oneplus_calendar" />
